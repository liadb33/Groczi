generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model chains {
  ChainId   String      @id @db.VarChar(50)
  ChainName String?     @db.VarChar(50)
  subchains subchains[]
}

model stores {
  ChainId    String    @db.VarChar(50)
  SubChainId String    @db.VarChar(50)
  StoreId    String    @db.VarChar(50)
  StoreType  Int?
  StoreName  String?   @db.VarChar(100)
  Address    String?   @db.VarChar(100)
  City       String?   @db.VarChar(50)
  ZipCode    String?   @db.VarChar(50)
  Lat        Decimal?  @db.Decimal(10, 0)
  Lon        Decimal?  @db.Decimal(10, 0)
  subchains  subchains @relation(fields: [ChainId, SubChainId], references: [ChainId, SubChainId], onDelete: NoAction, onUpdate: NoAction, map: "stores_ibfk_1")

  @@id([ChainId, SubChainId, StoreId])
}

model subchains {
  ChainId      String   @db.VarChar(50)
  SubChainId   String   @db.VarChar(50)
  SubChainName String?  @db.VarChar(50)
  stores       stores[]
  chains       chains   @relation(fields: [ChainId], references: [ChainId], onDelete: NoAction, onUpdate: NoAction, map: "subchains_ibfk_1")

  @@id([ChainId, SubChainId])
}
